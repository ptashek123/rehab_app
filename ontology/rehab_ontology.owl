        <hasSeverity rdf:datatype="&xsd;string">Легкая</hasSeverity>
        <hasAgeGroup rdf:datatype="&xsd;string">Взрослый</hasAgeGroup>
        <treatedBy rdf:resource="#specialist_neurologist"/>
        <treatedBy rdf:resource="#specialist_physiotherapist"/>
    </Patient>
    
    <Patient rdf:about="#patient_stroke_severe">
        <hasCondition rdf:datatype="&xsd;string">Инсульт</hasCondition>
        <hasSeverity rdf:datatype="&xsd;string">Тяжелая</hasSeverity>
        <hasAgeGroup rdf:datatype="&xsd;string">Пожилой</hasAgeGroup>
        <treatedBy rdf:resource="#specialist_neurologist"/>
        <treatedBy rdf:resource="#specialist_physiotherapist"/>
        <treatedBy rdf:resource="#specialist_ergotherapist"/>

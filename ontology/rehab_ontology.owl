<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/rehabilitation-ontology#"
     xml:base="http://www.semanticweb.org/rehabilitation-ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/rehabilitation-ontology"/>
    
    <owl:Class rdf:about="#Patient"/>
    <owl:Class rdf:about="#RehabilitationProgram"/>
    <owl:Class rdf:about="#RehabilitationMethod"/>
    <owl:Class rdf:about="#Specialist"/>
    
    <owl:ObjectProperty rdf:about="#hasCondition"/>
    <owl:ObjectProperty rdf:about="#hasSeverity"/>
    <owl:ObjectProperty rdf:about="#hasAgeGroup"/>
    
    <owl:ObjectProperty rdf:about="#includesMethod">
        <rdfs:domain rdf:resource="#RehabilitationProgram"/>
        <rdfs:range rdf:resource="#RehabilitationMethod"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#supervisedBy">
        <rdfs:domain rdf:resource="#RehabilitationProgram"/>
        <rdfs:range rdf:resource="#Specialist"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#suitableFor">
        <rdfs:domain rdf:resource="#RehabilitationProgram"/>
        <rdfs:range rdf:resource="#Patient"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#requiresSpecialist">
        <rdfs:domain rdf:resource="#RehabilitationMethod"/>
        <rdfs:range rdf:resource="#Specialist"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="#treatedBy">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Specialist"/>
    </owl:ObjectProperty>
    
    <owl:DatatypeProperty rdf:about="#hasDuration">
        <rdfs:domain rdf:resource="#RehabilitationProgram"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasSessionCount">
        <rdfs:domain rdf:resource="#RehabilitationProgram"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasEffectivenessScore">
        <rdfs:domain rdf:resource="#RehabilitationMethod"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    
    <Patient rdf:about="#patient_stroke_mild">

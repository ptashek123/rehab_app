        <treatedBy rdf:resource="#specialist_physiotherapist"/>
        <treatedBy rdf:resource="#specialist_lfk_doctor"/>
    </Patient>
    
    <RehabilitationProgram rdf:about="#program_neurological_mild">
        <hasDuration rdf:datatype="&xsd;integer">30</hasDuration>
        <hasSessionCount rdf:datatype="&xsd;integer">15</hasSessionCount>
        <includesMethod rdf:resource="#method_physiotherapy_electro"/>
        <includesMethod rdf:resource="#method_exercise_therapy"/>
        <supervisedBy rdf:resource="#specialist_neurologist"/>
        <supervisedBy rdf:resource="#specialist_physiotherapist"/>
        <suitableFor rdf:resource="#patient_stroke_mild"/>
        <suitableFor rdf:resource="#patient_post_surgery"/>
    </RehabilitationProgram>
    
    <RehabilitationProgram rdf:about="#program_neurological_severe">
